{
  "name": "@cordisjs/plugin-webui",
  "description": "Web User Interface for Koishi",
  "version": "0.3.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "browser": "./lib/browser.js",
      "default": "./lib/index.js"
    },
    "./shared": {
      "types": "./lib/shared/index.d.ts",
      "default": "./lib/shared/index.js"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "app",
    "lib",
    "dist"
  ],
  "author": "Shigma <shigma10826@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cordisjs/webui.git",
    "directory": "plugins/webui"
  },
  "bugs": {
    "url": "https://github.com/cordisjs/webui/issues"
  },
  "keywords": [
    "cordis",
    "plugin",
    "frontend",
    "webui",
    "console",
    "service"
  ],
  "cordis": {
    "description": {
      "en": "Web UI interface",
      "zh": "网页控制台"
    },
    "service": {
      "implements": [
        "webui"
      ]
    }
  },
  "peerDependencies": {
    "@cordisjs/client": "^0.3.1",
    "cordis": "^4.0.0-beta.1"
  },
  "peerDependenciesMeta": {
    "@cordisjs/client": {
      "optional": true
    }
  },
  "devDependencies": {
    "@cordisjs/client": "^0.3.1",
    "@cordisjs/plugin-server": "^1.2.0",
    "cordis": "^4.0.0-beta.1",
    "vite": "^6.2.2"
  },
  "dependencies": {
    "@cordisjs/fetch-file": "^1.0.3",
    "cosmokit": "^1.8.0",
    "es-module-lexer": "^1.6.0",
    "open": "^10.1.0"
  }
}
