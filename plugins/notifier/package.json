{
  "name": "@cordisjs/plugin-notifier",
  "description": "Notifier service for Cordis",
  "version": "0.2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.mjs"
    },
    "./src/*": "./src/*",
    "./client": "./client/index.ts",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "dist"
  ],
  "author": "Shigma <shigma10826@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cordiverse/webui.git",
    "directory": "plugins/notifier"
  },
  "bugs": {
    "url": "https://github.com/cordiverse/webui/issues"
  },
  "keywords": [
    "cordis",
    "plugin",
    "notifier",
    "webui"
  ],
  "cordis": {
    "public": [
      "dist"
    ],
    "description": {
      "en": "Notifier service for Cordis WebUI",
      "zh": "Cordis WebUI 通知服务"
    },
    "service": {
      "implements": [
        "notifier"
      ],
      "optional": [
        "manager",
        "webui"
      ]
    }
  },
  "peerDependencies": {
    "@cordisjs/plugin-webui": "^0.2.1",
    "cordis": "^4.0.0-alpha.5"
  },
  "devDependencies": {
    "@cordisjs/client": "^0.2.1",
    "@cordisjs/plugin-manager": "^0.4.0",
    "@cordisjs/plugin-webui": "^0.2.1",
    "cordis": "^4.0.0-alpha.5"
  },
  "dependencies": {
    "@cordisjs/element": "^0.1.0",
    "cosmokit": "^1.7.3"
  }
}
